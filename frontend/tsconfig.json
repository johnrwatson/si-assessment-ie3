{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "lib": ["esnext", "dom", "dom.iterable", "scripthost", "webworker"],
    "types": ["node"],
    "noImplicitAny": false
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
